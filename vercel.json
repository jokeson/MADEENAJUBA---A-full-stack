{
  "framework": "nextjs",
  "regions": ["iad1"],
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "functions": {
    "app/**/*.ts": {
      "maxDuration": 10
    },
    "app/**/*.tsx": {
      "maxDuration": 10
    }
  }
}

